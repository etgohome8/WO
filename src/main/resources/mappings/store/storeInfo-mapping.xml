<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SInfoDAO">
	<update id="updateTel">
		UPDATE STORE_INFO SET 
		SI_TEL = #{si_tel} 
		WHERE 
		SI_CODE=#{si_code} 
	</update>
	
</mapper>
 